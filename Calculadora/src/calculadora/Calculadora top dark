// SOMA +
    private void b_somaActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_b_somaActionPerformed
valor1 = Double.parseDouble(Visor.getText());
op = "soma"; // 1 = SOMA
Visor.setText("");
historico.setText(String.valueOf(valor1 + " +")); // HISTORICO RECEBE O VALOR1 E O SIMBOLO +
    }//GEN-LAST:event_b_somaActionPerformed
// MULTIPLICAÇÃO X
    private void b_multiplicaçãoActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_b_multiplicaçãoActionPerformed
valor1 = Double.parseDouble(Visor.getText());
Visor.setText("");
op = "multiplicacao"; // 2 = MULTIPLICAÇÃO
historico.setText(String.valueOf(valor1)+(" x "));// HISTORICO RECEBE O VALOR1 E O SIMBOLO X

    }//GEN-LAST:event_b_multiplicaçãoActionPerformed
// IGUAL =
    private void b_igualActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_b_igualActionPerformed
valor2 = Double.parseDouble(Visor.getText());
System.out.println("VALOR 1:" + valor1);
System.out.println("VALOR 2:" + valor2);
switch(op){
    case "soma": result = valor1+valor2;System.out.println("OPERAÇÃO + E RESULTADO:" + result);break;
    case "multiplicacao": result =valor1*valor2;System.out.println("OPERAÇÃO * E RESULTADO:" + result);break;   
    case "menos": result = valor1-valor2;System.out.println("OPERAÇÃO - E RESULTADO :" + result);break;
    case "divisao": result =valor1/valor2;System.out.println("OPERAÇÃO / E RESULTADO:" + result);break;
    default:Visor.setText("DEU RUIM");System.out.println("DEU RUIM");break;
} System.out.println("TESTE RESULTADO LINHA 699: " + result);
Visor.setText(String.valueOf(result));
historico.setText("");// APAGA HISTORICO
    }//GEN-LAST:event_b_igualActionPerformed

    private void b_menosActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_b_menosActionPerformed
valor1 = Double.parseDouble(Visor.getText());
op = "menos"; // 3 = MENOS
Visor.setText(""); // VISOR É APAGADO
historico.setText(String.valueOf(valor1 + " -")); // HISTORICO RECEBE VALOR1 E SIMBOLO -
    }//GEN-LAST:event_b_menosActionPerformed

